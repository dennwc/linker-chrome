{
  "manifest_version": 2,
  "name": "linker-chrome",
  "description": "linker-chrome",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"

  },
  "permissions": [
    "tabs",
    "http://localhost/*",
    "https://linker-nw.appspot.com/*"
  ]
}
